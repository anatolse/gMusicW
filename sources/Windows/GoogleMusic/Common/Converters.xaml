<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:OutcoldSolutions.GoogleMusic.Converters">

    <converters:StringFormatConverter x:Key="StringFormatConverter"/>
    <converters:StringFormatConverter x:Key="PercentageStringFormatConverter" Format="P0" />
    <converters:DoubleToTimeConverter x:Key="DoubleToTimeConverter"/>
    <converters:TimeSpanToTimeConverter x:Key="TimeSpanToTimeConverter" />
    <converters:AlbumArtUrlToImageConverter x:Key="AlbumArtUrlToImageConverter" />
    <converters:SizeToStringConverter x:Key="SizeToStringConverter" />
    <converters:SongToArtistConverter x:Key="SongToArtistConverter" />
    
</ResourceDictionary>
